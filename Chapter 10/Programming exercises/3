//find max in array-of-int
#include <stdio.h>
int max_of_array(int arr[],int n);
int main(void)
{
int arr[] = {1,2,3,4,5,6,7,8};

printf("The max of array is %d \n", max_of_array(arr, 8));

return 0;
}
int max_of_array(int arr[],int n)
{
    int i;
    int max;
    max = arr[0];
    for(i = 0; i < n; i++)
    {
        if(max < arr[i])
            max = arr[i];

    }

    return max;
}
